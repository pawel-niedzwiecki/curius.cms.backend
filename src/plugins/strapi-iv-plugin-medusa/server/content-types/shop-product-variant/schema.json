{
  "kind": "collectionType",
  "collectionName": "shop_product_variants",
  "info": {
    "displayName": "Shop product variant",
    "singularName": "shop-product-variant",
    "pluralName": "shop-product-variants",
    "tableName": "shop_product_variants"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "medusa_id": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "required": true
    },
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "deleted_at": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime",
      "required": false
    },
    "prices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::strapi-iv-plugin-medusa.shop-product-price-variant",
      "mappedBy": "medusa_product_variant"
    },
    "options": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::strapi-iv-plugin-medusa.shop-product-option-variant",
      "mappedBy": "medusa_product_variant"
    },
    "product_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::strapi-iv-plugin-medusa.shop-product",
      "inversedBy": "variants"
    },
    "stock_and_inventory": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "medusa.product-variant-stock-and-inventory",
      "required": true
    },
    "dimensions": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "medusa.product-variant-dimensions",
      "required": true
    },
    "Customs": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "medusa.product-variant-customs",
      "required": true
    }
  }
}
