{
  "kind": "collectionType",
  "collectionName": "medusa_products",
  "info": {
    "singularName": "medusa-product",
    "pluralName": "medusa-products",
    "displayName": "Medusa product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "handle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "discountable": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    },
    "images": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "component": "medusa.product-images"
    },
    "stock_and_inventory": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "medusa.product-stock-and-inventory",
      "required": true
    },
    "is_giftcard": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    },
    "medusa_profile_id": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid"
    },
    "medusa_id": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "required": true
    },
    "deleted_at": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime"
    },
    "option_variants_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::medusa-product-option-variant.medusa-product-option-variant",
      "mappedBy": "medusa_product_id"
    },
    "variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::medusa-product-variant.medusa-product-variant",
      "mappedBy": "product_id"
    },
    "collection": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::medusa-collection.medusa-collection",
      "inversedBy": "medusa_products"
    },
    "type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::medusa-product-type.medusa-product-type",
      "inversedBy": "products"
    },
    "options": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::medusa-product-option.medusa-product-option",
      "mappedBy": "product"
    }
  }
}
